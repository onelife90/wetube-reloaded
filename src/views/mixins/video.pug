mixin video(video)
    a(href=`/videos/${video.id}`).video-mixin
        div.video-mixin__thumb
            img.video-mixin__thumb(src=video.thumbUrl, crossorigin)
        div.video-mixin__data
            span.video-mixin__title=video.title
            div.video-mixin__meta
                span #{video.owner.name} â—½ 
                span #{video.meta.views} #{video.meta.views <=1 ? "view":"views"}